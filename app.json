{
  "expo": {
    "name": "happly-app",
    "slug": "happly-app",
    "version": "4.0.0",
    "orientation": "portrait",
    "icon": "./assets/happly.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/happly_splash.png",
      "resizeMode": "cover"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.engineeringwithjames.happlyapp",
      "buildNumber": "3.0.0",
      "config": {
        "usesNonExemptEncryption": false
      }
    },
    "android": {
      "package": "com.engineeringwithjames.happlyapp",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "versionCode": 1,
      "permissions": [
        "NOTIFICATIONS"
      ],
      "useNextNotificationsApi": true
    },
    "web": {
      "favicon": "./src/assets/happly.png"
    },
    "extra": {
      "eas": {
        "projectId": "f4a2e2e5-7b6d-4ff1-aeaf-da8279931695"
      }
    }
  }
}
