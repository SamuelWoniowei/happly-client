{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@env": [
        "node_modules/<path to the module dist>"
      ],
      "@assets/*": [
        "./src/assets/*"
      ],
      "@actions/*": [
        "./src/actions/*"
      ],
      "@components/*": [
        "./src/components/*"
      ],
      "@constant/*": [
        "./src/constant/*"
      ],
      "@db/*": [
        "./src/db/*"
      ],
      "@navigation/*": [
        "./src/navigation/*"
      ],
      "@screen/*": [
        "./src/screen/*"
      ],
      "@shared/*": [
        "./src/shared/*"
      ],
      "@state/*": [
        "./src/state/*"
      ],
      "@styles/*": [
        "./src/styles/*"
      ],
      "@types/*": [
        "./src/types/*"
      ],
      "@utils/*": [
        "./src/utils/*"
      ]
    }
  },
  "extends": "expo/tsconfig.base"
}
